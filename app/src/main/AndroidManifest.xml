<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mcm.edu.ph.rpginheritance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RPGinheritance">
        <activity android:name=".Troll"></activity>
        <activity android:name=".Goblin" />
        <activity android:name=".Orc" />
        <activity android:name=".Slime" />
        <activity android:name=".Mage" />
        <activity android:name=".Cleric" />
        <activity android:name=".Swordsman" />
        <activity android:name=".Archer" />
        <activity android:name=".monsterClasses" />
        <activity android:name=".heroClasses" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>